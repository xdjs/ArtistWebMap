{
  "version": 2,
  "buildCommand": "npm run build && ./copy-favicon.sh",
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "headers": {
        "Content-Type": "image/x-icon",
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}