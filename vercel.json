{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test"
    },
    {
      "src": "/api/artist-options/(.*)",
      "dest": "/api/artist-options/[artistName]"
    },
    {
      "src": "/api/network/(.*)",
      "dest": "/api/network/[artistName]"
    },
    {
      "src": "/api/search",
      "dest": "/api/search"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "functions": {
    "api/**/*.ts": "@vercel/node"
  },
  "env": {
    "NODE_ENV": "production"
  }
}